.nav{
    width: 100%;
    height: 125rpx;
    display: flex;
    position: relative;
    background-color: #fff;
}
.nav .point{
    width: 33%;
    text-align: center;
    line-height: 125rpx;
    font-size: 18px;
    color: #adabaf;
    transition-timing-function: cubic-bezier(0.1, 0.57, 0.1, 1);
    transition-duration: 1000ms;
}
.nav .point.hl{
    color: #f57d62;
}
.nav .bottomLine{
    flex: none;
    width:33%;
    height: 8rpx;
    background-color: #f57d62;
    position: absolute;
    left: 0rpx;
    bottom:0rpx;
    transition-timing-function: cubic-bezier(0.1, 0.57, 0.1, 1);
    transition-duration: 1000ms;
}
.SN{
    height:98rpx;
    line-height: 98rpx;
    background-color: #eeeeee;
    color: #333;
    font-size: 18px;
    text-align: center;
}
.sw{
    position: absolute;
    top:230rpx;
    bottom:0px;
    width: 100%;
    background-color: #fff;
}
.sw swiper{
    height: 100%;
}
.head{
    height:121rpx;
    width: 100%;
    border-bottom: 1px solid #acabb0;
}
.head .title{
    line-height: 121rpx;
    text-indent: 80rpx;
}
.netSet .list{
    width: 100%;
    display: flex;
    height:87rpx;
    margin-top: 40rpx;
}
.netSet .list .left{
    width: 30%;
    line-height: 87rpx;
    text-indent: 80rpx;
    padding-left: 20rpx;
    color: #adabb0;
}
.netSet .list .right{
    width: 60%;
    line-height: 87rpx;
    border: 1px solid #f57d62;
    text-indent: 20rpx;
}
.netSet .list .right input{
    width:100%;
    height:100%;
}
.netSet .bottom{
    width: 100%;
    display: flex;
    margin-top: 40rpx;
}
.netSet .bottom .left{
    width:30%;
    color: #adabb0;
    padding-left: 20rpx;
}
.netSet .bottom .right{
    width: 60%;
    line-height:30px;
    color: #adabb0;
}
.netSet .bottom icon{
    position: relative;
    top:10rpx;
    margin-right:20rpx;
}
.buttonFrame{
    margin-top: 20rpx;
    display: flex;
}
.buttonFrame button{
    width: 40%;
}
.listHead{
    width:100%;
    display: flex;
    position:relative;
    top: 30rpx;
    text-align: center;
    font-size: 30rpx;
}
.listHead .left{
    width:25%;
    border-right: 1px solid #adabb0;
}
.listHead .center{
    width:50%;
    border-right: 1px solid #adabb0;
}
.listHead .right{
    width:25%;
}
.noRub .list,.blackList .list{
    margin-top:64rpx;
    display: flex;
    font-size: 30rpx;
    text-align: center;
}
.noRub .list .left,.blackList .list .left{
    width:25%;
}
.noRub .list .center,.blackList .list .center{
    width:50%;
}
.noRub .list .right,.blackList .list .right{
    width:25%;
    color: #f57d62;
    text-decoration: underline;
}