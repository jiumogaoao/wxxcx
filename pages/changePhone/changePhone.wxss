.container .logo{
    display: block;
    margin: auto;
    margin-top:30rpx;
    width:175rpx;
    height:202rpx;
}
.listFrame{
    width: 90%;
    border: 1px solid #f57d62;
    background-color: #fff;
    margin: auto;
    margin-top: 20rpx;
    border-radius: 20rpx;
    overflow: hidden;
}
.list{
    width:100%;
    border-bottom: 1px solid #f57d62;
    display: flex;
    position:relative;
}
.list button{
    flex: none;
    position: absolute;
    top:6rpx;
    right:5rpx;
    width: 200rpx;
    height: 100rpx;
    font-size: 20rpx;
    line-height: 100rpx;
    padding: 0;
}
.list .left{
    width: 20%;
}
.list .right{
    width: 80%;
}
.list .right input{
    width: 100%;
    height: 100%;
}
.list .icon{
    width: 40rpx;
    height:112rpx;
    display: block;
    margin: auto;
}
.container .bottomButton{
    width:90%;
    margin-top: 40rpx;
}
.dsc{
    text-align: center;
    font-size: 12rpx;
    margin-top: 40rpx;
}
.red{
    display: inline;
    color: #f57d62;
    text-decoration: underline;
}